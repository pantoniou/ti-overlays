/dts-v1/;
/plugin/;

#include <dt-bindings/interrupt-controller/irq.h>

/ {
    fragment@1 {
        target = <&touchscreen>;
        __overlay__ {
            interrupt-parent = <&gpio1>;
            interrupts = <15 IRQ_TYPE_EDGE_FALLING>;
        };
    };
    fragment@2 {
        target = <&lcd>;
        __overlay__ {
            compatible = "osd,osd101t2587-53ts", "panel-dpi";
            panel-timing {
                clock-frequency = <164400000>;
                hactive = <1920>;
                vactive = <1200>;
                hfront-porch = <152>;
                hback-porch = <20>;
                hsync-len = <52>;
                vfront-porch = <24>;
                vback-porch = <48>;
                vsync-len = <6>;
                hsync-active = <0>;
                vsync-active = <0>;
                de-active = <1>;
                pixelclk-active = <1>;
            };
        };
    };
};
